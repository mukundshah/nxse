{
  "name": "nxse",
  "type": "module",
  "version": "0.1.2",
  "description": "Database utilities, API views, admin UI, and more for Nuxt",
  "license": "MIT",
  "homepage": "https://nxse.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/mukundshah/nxse"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "dev": "",
    "play": "pnpm dev",
    "dev:build": "",
    "dev:generate": "",
    "dev:prepare": "",
    "postinstall": "pnpm dev:prepare",
    "build": "unbuild",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm prepack && pnpm changelogen --release --push && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "h3": "npm:h3-nightly@latest",
    "mixwith.ts": "^1.1.3",
    "db0": "0.2.3",
    "valibot": "1.0.0-beta.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@types/node": "^22.12.0",
    "changelogen": "^0.5.7",
    "eslint": "^9.19.0",
    "unbuild": "3.3.1"
  }
}
