{
  "name": "nxse",
  "type": "module",
  "version": "0.1.2",
  "description": "Database utilities, API views, admin UI, and more for Nuxt",
  "license": "MIT",
  "homepage": "https://nxse.vercel.app",
  "configKey": "nxse",
  "compatibility": {
    "nuxt": "^3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mukundshah/nxse"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "play": "pnpm dev",
    "css": "tailwindcss -i ./tailwind.css -o ./src/runtime/assets/nxse-admin.min.css",
    "css:watch": "pnpm css --watch",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "pnpm css && nuxt-module-build prepare && nuxt-module-build build --stub && nuxi prepare playground",
    "postinstall": "pnpm dev:prepare",
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm prepack && pnpm changelogen --release --push && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.3",
    "@radix-icons/vue": "^1.0.0",
    "@tanstack/vue-table": "^8.11.8",
    "@vee-validate/zod": "^4.12.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-kit": "^0.20.13",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "knitwork": "^1.0.0",
    "pathe": "^1.1.2",
    "radix-vue": "^1.4.2",
    "scule": "1.2.0",
    "tailwind-merge": "^2.2.1",
    "ufo": "^1.3.2",
    "vee-validate": "^4.12.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.5.5",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "shadcn-vue": "^0.9.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
