{
  "name": "nxse",
  "type": "module",
  "version": "0.2.0",
  "description": "Database utilities, API views, admin UI, and more for Nuxt",
  "license": "MIT",
  "homepage": "https://nxse.vercel.app",
  "configKey": "nxse",
  "compatibility": {
    "nuxt": "^3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mukundshah/nxse"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "play": "pnpm dev",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build prepare && nuxt-module-build build --stub && nuxi prepare playground",
    "postinstall": "pnpm dev:prepare",
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm test && pnpm prepack && pnpm changelogen --release --push && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.3",
    "drizzle-kit": "^0.20.13",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "knitwork": "^1.0.0",
    "scule": "1.2.0",
    "sirv": "^2.0.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@iconify-json/carbon": "^1.1.28",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.3",
    "@nuxt/test-utils": "^3.10.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "nuxt": "^3.9.3"
  }
}
