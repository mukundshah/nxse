{
  "name": "nxse",
  "type": "module",
  "version": "0.1.2",
  "description": "Database utilities, API views, admin UI, and more for Nuxt",
  "license": "MIT",
  "homepage": "https://nxse.vercel.app",
  "configKey": "nxse",
  "compatibility": {
    "nuxt": "^3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mukundshah/nxse"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "play": "pnpm dev",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build prepare && nuxt-module-build build --stub && nuxi prepare playground",
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm prepack && pnpm changelogen --release --push && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-nightly@4.0.0-28726502.5de507f0",
    "@vinejs/vine": "^2.1.0",
    "h3": "npm:h3-nightly@1.12.1-1721739391.a15b8de",
    "knitwork": "^1.1.0",
    "mixwith.ts": "^1.1.3",
    "pathe": "^1.1.2",
    "scule": "1.3.0",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.1",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.8.3",
    "changelogen": "^0.5.5",
    "eslint": "^9.9.0"
  }
}
